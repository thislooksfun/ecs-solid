{
  "name": "ecs-solid",
  "scripts": {
    "build": "solid-start build",
    "clean": "rm -rf .solid .vercel dist",
    "db:schema:update": "prisma db push",
    "dev": "solid-start dev",
    "inspect": "serve -p 3001 .solid/inspect",
    "lint": "npm run lint:check",
    "lint:check": "npm-run-all --continue-on-error lint:check:*",
    "lint:check:eslint": "eslint \"**/*.{ts,tsx,js,jsx,cjs,mjs}\"",
    "lint:check:prettier": "prettier --check \"**/*\"",
    "lint:fix": "npm-run-all lint:fix:*",
    "lint:fix:eslint": "eslint --fix \"**/*.{ts,tsx,js,jsx,cjs,mjs}\"",
    "lint:fix:prettier": "prettier --write \"**/*\"",
    "prepare": "husky install",
    "postinstall": "patch-package && prisma generate",
    "postbuild": "./scripts/copy-prisma-to-vercel.sh",
    "start": "solid-start start",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "dependencies": {
    "@auth/core": "0.2.5",
    "@prisma/client": "4.8.1",
    "@solid-auth/next": "0.0.23",
    "@solidjs/meta": "0.28.2",
    "@solidjs/router": "0.6.0",
    "@upstash/ratelimit": "0.1.5",
    "@upstash/redis": "1.19.1",
    "dotenv": "16.0.3",
    "solid-icons": "1.0.4",
    "solid-js": "1.6.8",
    "solid-start": "0.2.11",
    "undici": "5.14.0",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.0",
    "@commitlint/config-conventional": "17.4.0",
    "@next-auth/prisma-adapter": "1.0.5",
    "@radix-ui/colors": "0.1.8",
    "@types/debug": "4.1.7",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "autoprefixer": "10.4.13",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-deprecation": "1.3.3",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-solid": "0.9.1",
    "eslint-plugin-sonarjs": "0.17.0",
    "eslint-plugin-tailwindcss": "3.8.0",
    "eslint-plugin-unicorn": "45.0.2",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "npm-run-all": "4.1.5",
    "patch-package": "6.5.1",
    "postcss": "8.4.21",
    "prettier": "2.8.2",
    "prisma": "4.8.1",
    "serve": "14.1.2",
    "solid-start-node": "0.2.11",
    "solid-start-vercel": "0.2.11",
    "tailwind-radix-colors": "0.0.9",
    "tailwindcss": "3.2.4",
    "tailwindcss-theme-swapper": "0.6.2",
    "typescript": "4.9.4",
    "vite": "3.2.5"
  },
  "overrides": {
    "@solid-primitives/props": "3.0.1"
  },
  "engines": {
    "node": "22.x"
  }
}
