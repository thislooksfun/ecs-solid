{
  "name": "ecs-solid",
  "scripts": {
    "build": "solid-start build",
    "clean": "rm -rf .solid .vercel dist",
    "db:schema:update": "prisma db push",
    "dev": "solid-start dev",
    "inspect": "serve -p 3001 .solid/inspect",
    "lint": "npm run lint:check",
    "lint:check": "npm-run-all --continue-on-error lint:check:*",
    "lint:check:eslint": "eslint \"**/*.{ts,tsx,js,jsx,cjs,mjs}\"",
    "lint:check:prettier": "prettier --check \"**/*\"",
    "lint:fix": "npm-run-all lint:fix:*",
    "lint:fix:eslint": "eslint --fix \"**/*.{ts,tsx,js,jsx,cjs,mjs}\"",
    "lint:fix:prettier": "prettier --write \"**/*\"",
    "prepare": "husky install",
    "postinstall": "patch-package && prisma generate",
    "postbuild": "cp node_modules/@prisma/engines/*query* .vercel/output/functions/render.func/ && cp prisma/schema.prisma .vercel/output/functions/render.func/",
    "start": "solid-start start",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "devDependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@radix-ui/colors": "0.1.8",
    "@types/debug": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "autoprefixer": "10.4.13",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-deprecation": "1.3.3",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.6",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-solid": "0.9.1",
    "eslint-plugin-sonarjs": "0.17.0",
    "eslint-plugin-tailwindcss": "3.7.1",
    "eslint-plugin-unicorn": "45.0.1",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "npm-run-all": "4.1.5",
    "patch-package": "6.5.1",
    "postcss": "8.4.19",
    "prettier": "2.8.2",
    "prisma": "4.7.1",
    "serve": "14.1.2",
    "solid-start-node": "0.2.7",
    "solid-start-vercel": "0.2.11",
    "tailwind-radix-colors": "^0.0.9",
    "tailwindcss": "3.2.4",
    "tailwindcss-theme-swapper": "0.6.2",
    "typescript": "4.9.4",
    "vite": "3.2.5"
  },
  "dependencies": {
    "@auth/core": "0.1.4",
    "@prisma/client": "4.7.1",
    "@solid-auth/next": "0.0.19",
    "@solidjs/meta": "0.28.2",
    "@solidjs/router": "0.5.1",
    "@upstash/ratelimit": "0.1.5",
    "@upstash/redis": "1.18.1",
    "dotenv": "16.0.3",
    "solid-icons": "^1.0.4",
    "solid-js": "1.6.4",
    "solid-start": "0.2.7",
    "undici": "5.14.0",
    "zod": "3.19.1"
  },
  "overrides": {
    "@solid-primitives/props": "3.0.1"
  },
  "engines": {
    "node": "^18"
  }
}
